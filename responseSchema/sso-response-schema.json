{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SSO Response Schema",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "token": {
          "type": "string"
        },
        "username": {
          "type": "string"
        }
      },
      "required": ["token", "username"]
    },
    "success": {
      "type": "boolean"
    }
  },
  "required": ["message", "success"]
}
